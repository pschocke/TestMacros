{
    "name": "pschocke/testmacros",
    "description": "Useful Laravel macros that extend tests",
    "license": "MIT",
    "authors": [
        {
            "name": "Patrick Schocke",
            "email": "patrick@ausbildung-ms.de",
            "homepage": "https://github.com/pschocke"
        }
    ],
    "homepage": "https://github.com/pschocke/testmacros",
    "keywords": ["Laravel", "TestMacros"],
    "require": {
        "laravel/framework": "~5.8.4|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.8|^4.0"
    },
    "autoload": {
        "psr-4": {
            "pschocke\\TestMacros\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "pschocke\\TestMacros\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "pschocke\\TestMacros\\TestMacrosServiceProvider"
            ]
        }
    }
}
